# Environment
ASPNETCORE_ENVIRONMENT=Development

# Database - connect to external SQL Server
SQL_SERVER_HOST=shared-sqlserver
SQL_SERVER_PASSWORD=YourStrong@Passw0rd
DB_NAME=ReampDb

# JWT
JWT_SECRET=YourSuperSecretKeyForJwtTokenGeneration32CharsMinimum!
JWT_ISSUER=Reamp.Api
JWT_AUDIENCE=Reamp.Client
JWT_ACCESS_TOKEN_EXPIRATION=60
JWT_REFRESH_TOKEN_EXPIRATION=7

# Cloudinary
CLOUDINARY_CLOUD_NAME=your_cloud_name
